            size_hint: 0.55, 0.10
            font_size: (root.width**2 + root.height**2) / 15**4
            text: "Przeglądaj raporty"
            bold: True
            color: 0.29, 0.29, 0.29, 1
            background_color: 0.85, 0.85, 0.85, 0.3
            on_release:
                root.manager.transition.direction = "up"
                app.root.current = "browser"
