<Browser>:
    name: "browser"
    id1: id1
    del_but: del_but
    layout_content: layout_content
    cancel_but: cancel_but
    BoxLayout:
        canvas.before:
            Color:
                rgba: 0.94, 0.89, 0.68, 1
