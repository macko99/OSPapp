#:set font_factor_3 0.38

<Browser>:
    name: "browser"
    id1: id1
    layout_content: layout_content
    cancel_but: cancel_but
    BoxLayout:
        canvas.before:
            Color:
                rgba: backg_color
            Rectangle:
                pos: self.pos
                size: self.size
        orientation: 'vertical'
        padding: 10, 10
        row_default_height: '48dp'
        row_force_default: True
        spacing: 10, 10
        ScrollView:
            size: self.size
            GridLayout:
                id: layout_content
                size_hint_y: None
                cols: 1
                row_default_height: '55dp'
                row_force_default: True
                spacing: [0, 5]
                padding: 0, 0
                Label:
                    id: id1
                    text: "Wybierz raport do edycji:"
                    font_size: self.height /1.8
                    color: color_font
                Button:
                    id: cancel_but
                    font_size: self.height * font_factor_3
                    text: "Anuluj"
                    color: color_font
                    background_normal: ''
                    background_color: color_button
                    background_down: color_dropdown
                    on_release:
                        root.cancel()