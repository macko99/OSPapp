        spacing: 10, 10
        ScrollView:
            size: self.size
            GridLayout:
                id: layout_content
                size_hint_y: None
                cols: 1
                row_default_height: '80dp'
                row_force_default: True
                spacing: 0, 0
                padding: 0, 0
                Label:
                    id: id1
                    text: "Wybierz raport do edycji:"
                    font_size: (root.width**2 + root.height**2) / 15**4
