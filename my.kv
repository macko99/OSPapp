<StartWindow>:
#    test:test
    FloatLayout:
        canvas.before:
            Color:
                rgba: 0.94, 0.89, 0.68, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text:"Raporty OSP"
            font_size: (root.width**2 + root.height**2) / 13**4
            pos_hint: {"x":0.25, "y":0.8}
            size_hint: 0.5, 0.2
            color: 0.29, 0.29, 0.29, 1
            bold: True
        Image:
            source: 'truck1.png'
            pos_hint: {"center_x":0.5, "center_y":0.65}
            size_hint: 0.3, 0.3
