<StartWindow>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 0.94, 0.89, 0.68, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text:"Raporty OSP"
            font_size: (root.width**2 + root.height**2) / 13**4
            pos_hint: {"x":0.25, "y":0.8}
            size_hint: 0.5, 0.2
            color: 0.29, 0.29, 0.29, 1
            bold: True
        Image:
            source: 'truck1.png'
            pos_hint: {"center_x":0.5, "center_y":0.65}
            size_hint: 0.3, 0.3
        Button:
            canvas.before:
                Color:
                    rgba: 0.85, 0.85, 0.85, 0.3
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [50,50]
            pos_hint:{"center_x":0.5,"center_y":0.5}
            size_hint: 0.55, 0.10
            font_size: (root.width**2 + root.height**2) / 15**4
            text: "Nowy raport"
            bold: True
            color: 0.29, 0.29, 0.29, 1
            background_color: 0.85, 0.85, 0.85, 0.3
            on_release:
                root.manager.transition.direction = "down"
                app.root.current = "create"
        Image:
            source: 'list2.png'
            pos_hint: {"center_x":0.5, "center_y":0.3}
            size_hint: 0.3, 0.2
        Button:
            canvas.before:
                Color:
                    rgba: 0.85, 0.85, 0.85, 0.3
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [50,50]
            pos_hint:{"center_x":0.5,"center_y":0.15}
            size_hint: 0.55, 0.10
            font_size: (root.width**2 + root.height**2) / 15**4
            text: "Przeglądaj raporty"
            bold: True
            color: 0.29, 0.29, 0.29, 1
            background_color: 0.85, 0.85, 0.85, 0.3
            on_release:
                root.manager.transition.direction = "up"
                app.root.current = "browser"

