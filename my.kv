            font_size: (root.width**2 + root.height**2) / 15**4
            pos_hint: {"x":0.3, "y":0.5}
            size_hint: 0.4, 0.2
        Button:
            pos_hint:{"x":0.3,"y":0.1}
            size_hint: 0.4, 0.1
            font_size: (root.width**2 + root.height**2) / 15**4
            text: 'Zamknij'
            on_release: root.dismiss()

<readyPopout@Popup>:
    auto_dismiss: False
    title: "Uwaga!"
    size: 400, 400
    FloatLayout:
        Label:
